  "invocaciones_arcanas": [
    {"nombre": "Explosión Agonizante", "requisito": "Nivel 2+, truco Warlock dañino", "efecto": "Elige truco Warlock dañino. Añade Mod Carisma a tiradas daño.", "repetible": true, "referencia": "[sources 244-246]"},
    {"nombre": "Armadura de Sombras", "efecto": "Puedes lanzar Armadura Magos sobre ti sin ranura.", "referencia": "[source 247]"},
    {"nombre": "Paso Ascendente", "requisito": "Nivel 5+", "efecto": "Puedes lanzar Levitar sobre ti sin ranura.", "referencia": "[source 248]"},
    {"nombre": "Visión Infernal", "requisito": "Nivel 2+", "efecto": "Ves normal en Luz Tenue/Oscuridad (mágica/no mágica) a 120 pies.", "referencia": "[source 249]"},
    {"nombre": "Espada Devoradora", "requisito": "Nivel 12+, Invocación Espada Sedienta", "efecto": "Ataque Adicional de Espada Sedienta otorga 2 ataques extra (en vez 1).", "referencia": "[source 250]"},
    {"nombre": "Mente Arcana", "efecto": "Ventaja en TS Constitución para mantener Concentración.", "referencia": "[source 251]"},
    {"nombre": "Golpe Arcano", "requisito": "Nivel 5+, Invocación Pacto Espada", "efecto": "1/turno al golpear con arma pacto, gasta ranura Magia Pacto, causa 1d8 Fuerza extra + 1d8/nivel ranura. Puedes Derribar (Prone) si G o menor.", "referencia": "[source 252]"},
    {"nombre": "Lanza Arcana", "requisito": "Nivel 2+, truco Warlock dañino alcance 10+", "efecto": "Elige truco Warlock dañino (alcance 10+). Alcance aumenta 30 * Nivel Warlock pies.", "repetible": true, "referencia": "[sources 253-255]"},
    {"nombre": "Vigor Infernal", "requisito": "Nivel 2+", "efecto": "Puedes lanzar Vida Falsa sobre ti sin ranura. Ganas PG Temp máx. (no tiras dado).", "referencia": "[sources 256-258]"},
    {"nombre": "Mirada de Dos Mentes", "requisito": "Nivel 5+", "efecto": "Acción Bonus, toca criatura dispuesta. Percibes por sus sentidos hasta fin tu sig. turno. Acción Bonus para mantener conexión. Mientras conectado y mismo plano: usas sus sentidos especiales, puedes lanzar conjuros desde tu espacio o el suyo (si a 60 pies).", "referencia": "[sources 259-262]"},
    {"nombre": "Regalo de las Profundidades", "requisito": "Nivel 5+", "efecto": "Respiras bajo agua, Vel. Nado = tu Velocidad. Lanza Respirar Bajo Agua 1 vez/Descanso Largo sin ranura.", "referencia": "[sources 263-265]"},
    {"nombre": "Regalo de los Protectores", "requisito": "Nivel 9+, Invocación Pacto Tomo", "efecto": "Página extra en Libro Sombras. Criatura (con permiso) escribe nombre (máx Mod Carisma nombres). Si criatura a 0 PG (no muere), baja a 1 PG. 1 activación/Descanso Largo. Acción mágica para borrar nombre.", "referencia": "[sources 266-270]"},
    {"nombre": "Inversión del Maestro de la Cadena", "requisito": "Nivel 5+, Invocación Pacto Cadena", "efecto": "Al lanzar Encontrar Familiar, familiar gana beneficios: Vel. Vuelo O Nado 40 pies; Acción Bonus para ordenar Ataque; Daño puede ser Necrótico/Radiante; Usa tu CD salvación hechizos; Reacción para darle Resistencia a daño recibido.", "referencia": "[sources 271-276]"},
    {"nombre": "Lecciones de los Primeros", "requisito": "Nivel 2+", "efecto": "Obtienes 1 dote Origen a elección.", "repetible": true, "referencia": "[sources 277-278]"},
    {"nombre": "Chupador de Vida", "requisito": "Nivel 9+, Invocación Pacto Espada", "efecto": "1/turno al golpear con arma pacto, causa 1d6 Necrótico/Psíquico/Radiante extra (tu elección). Puedes gastar Dado Golpe para recuperar PG = tirada + Mod Con.", "referencia": "[source 279]"},
    {"nombre": "Máscara de Muchas Caras", "requisito": "Nivel 2+", "efecto": "Puedes lanzar Disfrazarse sin ranura.", "referencia": "[source 280]"},
    {"nombre": "Maestro de las Mil Formas", "requisito": "Nivel 5+", "efecto": "Puedes lanzar Adivinar sin ranura.", "referencia": "[source 281]"},
    {"nombre": "Visiones Nebulosas", "requisito": "Nivel 2+", "efecto": "Puedes lanzar Imagen Silenciosa sin ranura.", "referencia": "[source 282]"},
    {"nombre": "Uno con las Sombras", "requisito": "Nivel 5+", "efecto": "Mientras en Luz Tenue/Oscuridad, puedes lanzar Invisibilidad sobre ti sin ranura.", "referencia": "[source 283]"},
    {"nombre": "Salto Sobrenatural", "requisito": "Nivel 2+", "efecto": "Puedes lanzar Salto sobre ti sin ranura.", "referencia": "[source 284]"},
    {"nombre": "Pacto de la Espada", "efecto": "Acción Bonus, conjura arma pacto (Simple/Marcial CC) O vincula arma mágica (si no sintonizada/vinculada). Competente con ella, úsala como Foco. Usa Mod Carisma para ataque/daño. Daño puede ser Necrótico/Psíquico/Radiante o normal. Vínculo termina si reusas Acción Bonus, arma > 5 pies 1 min, o mueres. Arma conjurada desaparece.", "referencia": "[sources 285-292]"},
    {"nombre": "Pacto de la Cadena", "efecto": "Aprendes Encontrar Familiar, puedes lanzarlo acción mágica sin ranura. Formas adicionales: Imp, Pseudodragón, Quasít, Esqueleto, Renacuajo Slaad, Esfinge Maravillas, Hada, Serpiente Venenosa. Al tomar acción Ataque, renuncia 1 ataque para permitir familiar Reacción atacar.", "referencia": "[sources 292-294]"},
    {"nombre": "Pacto del Tomo", "efecto": "Conjura Libro Sombras (tu apariencia) en Descanso Corto/Largo. Contiene magia. Desaparece si conjuras otro/mueres. Beneficios: Elige 3 trucos + 2 conjuros nivel 1 Ritual (cualquier lista, no preparados). Mientras libro contigo, tienes esos conjuros preparados (cuentan Warlock). Puedes usar libro como Foco.", "referencia": "[sources 295-301]"},
    {"nombre": "Explosión Repulsiva", "requisito": "Nivel 2+, truco Warlock dañino tirada ataque", "efecto": "Elige truco Warlock tirada ataque. Al acertar criatura G o menor, puedes empujar 10 pies.", "repetible": true, "referencia": "[sources 301-303]"},
    {"nombre": "Espada Sedienta", "requisito": "Nivel 5+, Invocación Pacto Espada", "efecto": "Ganas Ataque Extra solo para arma pacto.", "referencia": "[sources 304-305]"},
    {"nombre": "Visiones de Reinos Distantes", "requisito": "Nivel 9+", "efecto": "Puedes lanzar Ojo Arcano sin ranura.", "referencia": "[source 306]"},
    {"nombre": "Susurros de la Tumba", "requisito": "Nivel 7+", "efecto": "Puedes lanzar Hablar con Muertos sin ranura.", "referencia": "[source 307]"},
    {"nombre": "Vista de Bruja", "requisito": "Nivel 15+", "efecto": "Visión Verdadera 30 pies.", "referencia": "[source 308]"}
  ],
